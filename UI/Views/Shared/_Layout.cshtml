<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title</title>
    <meta name="description" content="Bootstrap menu over effect dropdown and click opening.">
    <script src="~/HTWF/scripts/jquery.min.js"></script>
    <link rel="stylesheet" href="~/HTWF/scripts/bootstrap/css/bootstrap.css">
    <script src="~/HTWF/scripts/script.js"></script>
    <link rel="stylesheet" href="~/HTWF/style.css">
    <link rel="stylesheet" href="~/HTWF/scripts/flexslider/flexslider.css">
    <link rel="stylesheet" href="~/HTWF/css/content-box.css">
    <link rel="stylesheet" href="~/HTWF/css/components.css">
    <link rel="stylesheet" href="~/HTWF/css/animations.css">
    <link rel="stylesheet" href="~/HTWF/scripts/php/contact-form.css">
    <link rel="stylesheet" href="~/HTWF/scripts/social.stream.css">
    <link rel="icon" style="height:100%" href="~/images/logos/dyt.png">
    <link rel="stylesheet" href="~/HTWF/skin.css">
    <link rel="stylesheet" href="~/HTWF/scripts/magnific-popup.css">
    <link rel="stylesheet" href="../../../HTWF/scripts/jquery.flipster.min.css">


</head>
<body>
    <div id="preloader"></div>
    <header data-menu-anima="fade-in">
        <div class="navbar navbar-default" role="navigation">
            <div class="navbar-mini">
                <div class="container">
                    <div class="nav navbar-nav navbar-left">
                        <hr />
                        <span><i class="fa fa-envelope-o"></i>info@diyetisyenmeryemyakupoglu.com</span>
                        <hr />
                    </div>
                    <div class="nav navbar-nav navbar-right">
                        <div class="minisocial-group">
                            <a asp-controller="ProvidedService" asp-action="Index"><i class="fa fa-user"></i></a>
                        </div>
                    </div>
                </div>
            </div>
            <div class="navbar navbar-main mega-menu-fullwidth">
                <div class="container">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle">
                            <i class="fa fa-bars"></i>
                        </button>
                        <a class="navbar-brand" asp-action="Index"><img src="~/images/logos/dytyazi.png" alt="logo" /> </a>
                    </div>
                    <div class="collapse navbar-collapse">
                        <ul class="nav navbar-nav">
                            <li><a asp-action="AboutUs">Hakkımızda</a></li>
                            @*<li><a asp-action="Services">Hizmetler</a></li>*@
                            <li><a asp-action="Blog">Blog</a></li>
                            @*<li><a asp-action="Pricing">Diyet Paketleri</a></li>*@
                            <li><a asp-action="OnlineDiet">Online Diyet</a></li>
                            <li><a asp-action="Weakened">Zayıflayanlar</a></li>
                            <li><a asp-action="Comments">Yorumlar</a></li>
                            <li><a asp-action="Contact">Bize Ulaş</a></li>
                        </ul>
                        <div class="nav navbar-nav navbar-right">
                            <ul class="nav navbar-nav">
                                <li><a class="btn" style="background-color: #A9CCE3 !important; color:white !important;" asp-action="OnlineDietRecord">Hemen Başlayalım</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </header>
    <div>
        @RenderBody()
    </div>

    <footer class="footer-base">
        <div class="content">
            <div class="container">
                <div class="row">
                    <div class="col-md-3 footer-center text-left">
                        <img width="100%" src="~/images/logos/dytyazilight.png" alt="" />
                    </div>
                    <div class="col-md-6 footer-left text-center">
                        <div class="tag-row">
                            <span><a href="mailto:destek@diyetisyenmeryemyakupoglu.com">Destek</a></span>
                            <span><a asp-action="AboutUs">SSS</a></span>
                            <span><a asp-action="Contact">İletişim</a></span>
                        </div>
                    </div>
                    <div class="col-md-3 footer-center text-right text-left-sm">
                        <img width="100%" src="~/images/logos/quarter.png" alt="" />
                    </div>
                </div>
            </div>
            <div class="row copy-row">
                <div class="col-md-12 copy-text">
                    © Copyright Diyetisyenmeryemyakupoglu. All Rights Reserved <span>Designed and developed by <a href="mailto:yunusyakupoglu@outlook.com">Yunus YAKUPOĞLU</a></span>
                </div>
            </div>
        </div>
    </footer>
    <link rel="stylesheet" href="~/HTWF/scripts/font-awesome/css/font-awesome.min.css">
    <script src="~/HTWF/scripts/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/HTWF/scripts/flexslider/jquery.flexslider-min.js"></script>
    <script src="~/HTWF/scripts/jquery.tab-accordion.js"></script>
    <script src="~/HTWF/scripts/parallax.min.js"></script>
    <script src="~/HTWF/scripts/imagesloaded.min.js"></script>
    <script src="~/HTWF/scripts/isotope.min.js"></script>
    <script src="~/HTWF/scripts/jquery.twbsPagination.min.js"></script>
    <script src="~/HTWF/scripts/jquery.magnific-popup.min.js"></script>
    <script src="~/HTWF/scripts/smooth.scroll.min.js"></script>
    <script src="~/HTWF/scripts/google.maps.min.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?sensor=false"></script>
    <script src="~/HTWF/scripts/social.stream.min.js"></script>
    <script src="~/HTWF/scripts/flexslider/jquery.flexslider-min.js"></script>
    <script src="../../../HTWF/scripts/jquery.flipster.min.js"></script>
    <script src="../../../HTWF/scripts/jquery.progress-counter.js"></script>
    @await RenderSectionAsync("Scripts", required: false)

</body>
</html>
